{
  "version": 2,
  "builds": [
    {
      "src": "api/ejecutar.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/ejecutar",
      "dest": "/api/ejecutar"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
